         </div>

      </div>

   </section>  -->


   <!-- Testimonials Section
   ================================================== -->
   <section id="testimonials">

      <a class="twitter-timeline" data-width="600" data-height="450" href="https://twitter.com/lelelthelit">Tweets by lifehacker</a> <script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>

   </section> <!-- Testimonials Section End-->


   <!-- Contact Section
   ================================================== -->
   <section id="contact">

         <div class="row section-head">

            <div class="two columns header-col">

               <h1><span>Get In Touch.</span></h1>

            </div>

            <div class="ten columns">

                  <p class="lead"> GET IN TOUCH!
                  </p>

            </div>

         </div>

         <div class="row">

            <div class="eight columns">

               <!-- form -->
               <form action="" method="post" id="contactForm" name="contactForm">
          <fieldset>

                  <div>
               <label for="contactName">Name <span class="required">*</span></label>
               <input type="text" value="" size="35" id="contactName" name="contactName">
                  </div>

                  <div>
               <label for="contactEmail">Email <span class="required">*</span></label>
               <input type="text" value="" size="35" id="contactEmail" name="contactEmail">
                  </div>

                  <div>
               <label for="contactSubject">Subject</label>
               <input type="text" value="" size="35" id="contactSubject" name="contactSubject">
                  </div>

                  <div>
                     <label for="contactMessage">Message <span class="required">*</span></label>
                     <textarea cols="50" rows="15" id="contactMessage" name="contactMessage"></textarea>
                  </div>

                  <div>
                     <button class="submit">Submit</button>
                     <span id="image-loader">
                        <img alt="" src="/images/loader.gif">
                     </span>
                  </div>

          </fieldset>
           </form> <!-- Form End -->

               <!-- contact-warning -->
               <div id="message-warning"> Error boy</div>
               <!-- contact-success -->
           <div id="message-success">
                  <i class="fa fa-check"></i>Your message was sent, thank you!<br>
           </div>

            </div>


            

      </div>

   </section> <!-- Contact Section End-->


   <!-- footer
   ================================================== -->
   <footer>

      <div class="row">

         <div class="twelve columns">

            <ul class="social-links">
               <li><a title="Github" href="{@github_url}"><i class="fa fa-github"></i></a></li>
               <li><a title="Twitter" href="https://twitter.com/#{@twitter}"><i class="fa fa-twitter"></i></a></li>
               <li><a title="Blog" href="{@personal_blog_url}"><i class="fa fa-file-text-o"></i></a></li>
               <li><a title="LinkedIn" href="{@linkedin_url}"><i class="fa fa-linkedin"></i></a></li>
               <li><a title="Resume" href="{@online_resume_url}"><i class="fa fa-pencil-square-o"></i></a>
            </ul>

            <ul class="copyright">
               <li>&copy; Copyright 2014 CeeVee</li>
               <li>Design by <a title="Styleshout" href="http://www.styleshout.com/">Styleshout</a></li>   
            </ul>

         </div>

         <div id="go-top"><a class="smoothscroll" title="Back to Top" href="#home"><i class="icon-up-open"></i></a></div>

      </div>


   </footer> <!-- Footer End-->

